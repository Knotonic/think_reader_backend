{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "outDir": "dist",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "strictNullChecks": false
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
   
    "node_modules",
  ]
}